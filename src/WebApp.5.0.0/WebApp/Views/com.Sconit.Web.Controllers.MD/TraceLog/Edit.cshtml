@using com.Sconit.Entity.SYS
@model com.Sconit.Entity.MD.TraceLog
@{
    Html.RenderAction("_SiteMapPath", "Common", new { MenuContent = "Url_TraceLog_View" });
}

@using (Html.BeginForm())
{
    <fieldset>
        <legend>@Resources.MD.Currency.Currency_Edit_Title</legend>
        <div class="editor">
            @Html.HiddenFor(model => model.Id)
            <div class="editor-label-2">
                <label>表名</label>
            </div>
            <div class="editor-field-2">
                @Html.DisplayFor(model => model.EntityTable)
            </div>
            <div class="editor-label-2">
                <label>操作</label>
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Operator)
            </div>
            <div class="editor-label-2">
                <label>主键</label>
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.Key1)
            </div>
            <div class="editor-label-2">
                <label>操作人</label>
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.UserName)
            </div>
            <div class="editor-label-2">
                <label>操作日期</label>
            </div>
            <div class="editor-field-2">
                @Html.EditorFor(model => model.OperateDate)
            </div>
        </div>
        <div class="editor-footer">
            <button type="button" id="btnBack" onclick="location.href='@Url.Content("~/TraceLog/List")'">
                @Resources.SYS.Global.Button_Back</button>
        </div>
    </fieldset>
}
@{
    Html.RenderAction("_DetailList", "TraceLog", new { id = Model.Id});
}